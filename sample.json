  {
    "id": "6ca2aa61-36cb-4f7b-89b5-bd568ff50345",
    "name": "Angular: Tour of Heroes",
    "desc": "At high level, this engine adds custom logic on top of Angular's \"Tour of Heroes\" tutorial application.  Calculate top heroes.  Make some heroes read only and protect some other heroes from being removed.  \n\nhttps://rt.qiangc.net/help/demo/angular-tour-of-heroes",
    "tags": ["demo", "logic injection"],
    "shareId": "",
    "syncId": "",
    "rules": [
      {
        "name": "Top heroes",
        "desc": "If heroes name start with Dr., then mark it as top hero",
        "action": {
          "annotate": "top hero"
        },
        "templates": [
          {
            "name": "^Dr"
          }
        ]
      },
      {
        "name": "Keep",
        "desc": "Prevent been deleted",
        "action": {
          "annotate": "keep"
        },
        "templates": [
          {
            "id": "(13,15)"
          }
        ]
      },
      {
        "name": "Read only",
        "desc": "Update is not allowed.",
        "action": {
          "annotate": "readonly"
        },
        "templates": [
          {
            "name": "Magneta|Dynama"
          }
        ]
      }
    ]
  },
